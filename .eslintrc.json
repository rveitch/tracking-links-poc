{
  "root": true,
  "env": {
    "es2022": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "import/extensions": ["error", { "js": "ignorePackages" }],
    "max-len": "off",
    "no-use-before-define": ["error", { "functions": false }],
    "no-underscore-dangle": "off",
    "no-console": "off"
  }
}
